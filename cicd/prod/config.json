{
  "app": {
    "name": "diversity-and-inclusion"
  },
  "build": {
    "dockerFileDirectory": "./dockerfiles/dni-frontend_docker_build_prod.Dockerfile",
    "args": {
      "NEXUS_ACCESS_TOKEN": {
        "type": "secret",
        "name": "nexus_access_token"
      },
      "NODE_ENV": {
        "type": "plain",
        "value": "production"
      },
      "REACT_APP_API_URL": {
        "type": "plain",
        "value": "/api"
      },
      "REACT_APP_WS_URL": {
        "type": "plain",
        "value": "/socket.io"
      },
      "REACT_APP_OURTESCO_URL": {
        "type": "plain",
        "value": "https://www.ourtesco.com"
      },
      "PUBLIC_URL": {
        "type": "plain",
        "value": "/diversity-and-inclusion"
      }
    }
  },
  "deployment": {
    "envs": {
      "NODE_ENV": {
        "type": "plain",
        "value": "production"
      },
      "NODE_CONFIG_ENV": {
        "type": "plain",
        "value": "prod"
      },
      "NODE_PORT": {
        "type": "plain",
        "value": "9000"
      },
      "PUBLIC_URL": {
        "type": "plain",
        "value": "/diversity-and-inclusion"
      },
      "DNI_APP_URL": {
        "type": "plain",
        "value": "https://www.ourtesco.com"
      },
      "DNI_APP_POST_URL": {
        "type": "plain",
        "value": "/network-news/%ID%"
      },
      "DNI_APP_EVENT_URL": {
        "type": "plain",
        "value": "/events/%ID%"
      },
      "DNI_APP_UNSUBSCRIBE_URL": {
        "type": "plain",
        "value": "/notification/settings?unsubscribe=true"
      },
      "MAIL_TEMPLATE_ID": {
        "type": "plain",
        "value": "dfge2b6d-qfaa-47c6-bfw9-b4214y0a2e88"
      },
      "MAIL_SEND_CHUNKS_NUMBER": {
        "type": "plain",
        "value": "30"
      },
      "WITH_ONE_LOGIN": {
        "type": "plain",
        "value": "true"
      },
      "OIDC_CLIENT_ID": {
        "type": "secret",
        "name": "oidc_client_id"
      },
      "OIDC_CLIENT_SECRET": {
        "type": "secret",
        "name": "oidc_client_secret"
      },
      "ISSUER_URL": {
        "type": "plain",
        "value": "https://login.ourtesco.com/oidc/2"
      },
      "REGISTERED_CALLBACK_URL_ROOT": {
        "type": "plain",
        "value": "https://www.ourtesco.com"
      },
      "REGISTERED_CALLBACK_URL_PATH": {
        "type": "plain",
        "value": "/auth/openid/callback"
      },
      "REDIRECT_AFTER_LOGOUT_URL": {
        "type": "plain",
        "value": "https://login.ourtesco.com/logout"
      },
      "REFRESH_TOKEN_SECRET": {
        "type": "secret",
        "value": "refresh_token_secret"
      },
      "IDENTITY_CLIENT_ID": {
        "type": "secret",
        "value": "identity_client_id"
      },
      "IDENTITY_CLIENT_SECRET": {
        "type": "secret",
        "value": "identity_client_secret"
      },
      "IDENTITY_USER_SCOPED_TOKEN_COOKIE_SECRET": {
        "type": "secret",
        "value": "identity_user_scoped_token_cookie_secret"
      },
      "IDENTITY_USER_SCOPED_TOKEN_COOKIE_NAME": {
        "type": "plain",
        "value": "identity-user-token"
      },
      "TYPEORM_TYPE": {
        "type": "plain",
        "value": "postgres"
      },
      "TYPEORM_HOST": {
        "type": "plain",
        "value": "eun-prod-213-dai-dipostgres.postgres.database.azure.com"
      },
      "TYPEORM_PORT": {
        "type": "plain",
        "value": "5432"
      },
      "TYPEORM_DATABASE": {
        "type": "plain",
        "value": "di_database"
      },
      "TYPEORM_SCHEMA": {
        "type": "plain",
        "value": "dni"
      },
      "TYPEORM_USERNAME": {
        "type": "secret",
        "name": "typeorm_username"
      },
      "TYPEORM_PASSWORD": {
        "type": "secret",
        "name": "typeorm_password"
      },
      "TYPEORM_SSL": {
        "type": "plain",
        "value": "true"
      },
      "TYPEORM_SYNCHRONIZE": {
        "type": "plain",
        "value": "false"
      },
      "TYPEORM_LOGGING": {
        "type": "plain",
        "value": "false"
      },
      "TYPEORM_RUN_MIGRATION": {
        "type": "plain",
        "value": "true"
      },
      "CCRM_RUN_SYNC": {
        "type": "plain",
        "value": "true"
      },
      "CACHE_COLLEAGUE_TTL": {
        "type": "plain",
        "value": "43200"
      },
      "COOKIE_SESSION_KEY": {
        "type": "plain",
        "value": "dni_cookie"
      },
      "COOKIE_USER_KEY": {
        "type": "plain",
        "value": "user_data"
      },
      "OIDC_GROUPS_ADMIN_ROLE": {
        "type": "plain",
        "value": "GG-UK-TescoGlobal-DiversityAndInclusion-Admin"
      },
      "OIDC_GROUPS_MANAGER_ROLE": {
        "type": "plain",
        "value": "GG-UK-TescoGlobal-DiversityAndInclusion-Manager"
      }
    }
  }
}
