{
  "app": {
    "name": "diversity-and-inclusion"
  },
  "build": {
    "dockerFileDirectory": "./dockerfiles/dni-frontend_docker_multistage_build.Dockerfile",
    "args": {
      "NPM_ACCESS_TOKEN": {
        "type": "secret",
        "name": "npm_access_token"
      },
      "NODE_ENV": {
        "type": "plain",
        "value": "ppe"
      },
      "REACT_APP_API_URL": {
        "type": "plain",
        "value": "/api"
      },
      "REACT_APP_WS_URL": {
        "type": "plain",
        "value": "/socket.io"
      },
      "REACT_APP_OURTESCO_URL": {
        "type": "plain",
        "value": "https://ppe.ourtesco.com"
      },
      "REACT_APP_LOGOUT_URL": {
        "type": "plain",
        "value": "/sso/logout"
      },
      "PUBLIC_URL": {
        "type": "plain",
        "value": "/dev-thp/diversity-and-inclusion"
      }
    }
  },
  "deployment": {
    "envs": {
      "NODE_ENV": {
        "type": "plain",
        "value": "ppe"
      },
      "RUNTIME_ENV": {
        "type": "plain",
        "value": "ppe"
      },
      "NODE_PORT": {
        "type": "plain",
        "value": "9000"
      },
      "APPLICATION_PUBLIC_URL": {
        "type": "plain",
        "value": "/dev-thp/diversity-and-inclusion"
      },
      "APPLICATION_SERVER_URL_ROOT": {
        "type": "plain",
        "value": "https://diversity-and-inclusion.dev.thp.tescocloud.com"
      },
      "APPLICATION_URL_TEMPLATE_POST": {
        "type": "plain",
        "value": "/network-news/%ID%"
      },
      "APPLICATION_URL_TEMPLATE_EVENT": {
        "type": "plain",
        "value": "/events/%ID%"
      },
      "APPLICATION_URL_UNSUBSCRIBE": {
        "type": "plain",
        "value": "/notification/settings?unsubscribe=true"
      },
      "APPLICATION_COLLEAGUE_DATA_COOKIE_NAME": {
        "type": "plain",
        "value": "dni.colleague"
      },
      "APPLICATION_COLLEAGUE_DATA_COOKIE_SECRET": {
        "type": "secret",
        "name": "application_colleague_data_cookie_secret"
      },
      "APPLICATION_USER_DATA_COOKIE_NAME": {
        "type": "plain",
        "value": "dni.user.data"
      },
      "APPLICATION_USER_DATA_COOKIE_SECRET": {
        "type": "secret",
        "name": "application_user_data_cookie_secret"
      },
      "APPLICATION_COOKIE_PARSER_SECRET": {
        "type": "secret",
        "name": "application_cookie_parser_secret"
      },
      "APPLICATION_WEB_HOOKS_SECRET": {
        "type": "secret",
        "name": "application_web_hooks_secret"
      },
      "MAILING_NEW_ENTITY_TEMPLATE_ID": {
        "type": "plain",
        "value": "dfge2b6d-qfaa-47c6-bfw9-b4214y0a2e88"
      },
      "MAILING_CHUNK_SIZE": {
        "type": "plain",
        "value": "30"
      },
      "USE_ONELOGIN": {
        "type": "plain",
        "value": "true"
      },
      "OIDC_CLIENT_ID": {
        "type": "secret",
        "name": "oidc_client_id"
      },
      "OIDC_CLIENT_SECRET": {
        "type": "secret",
        "name": "oidc_client_secret"
      },
      "OIDC_REFRESH_TOKEN_SECRET": {
        "type": "secret",
        "name": "oidc_refresh_token_secret"
      },
      "OIDC_ISSUER_URL": {
        "type": "plain",
        "value": "https://loginppe.ourtesco.com/oidc/2"
      },
      "OIDC_AUTH_CALLBACK_PATH": {
        "type": "plain",
        "value": "/sso/auth/callback"
      },
      "OIDC_REDIRECT_AFTER_LOGOUT_CALLBACK_PATH": {
        "type": "plain",
        "value": "/sso/logout/callback"
      },
      "IDENTITY_CLIENT_ID": {
        "type": "secret",
        "name": "identity_client_id"
      },
      "IDENTITY_CLIENT_SECRET": {
        "type": "secret",
        "name": "identity_client_secret"
      },
      "IDENTITY_USER_SCOPED_TOKEN_COOKIE_NAME": {
        "type": "plain",
        "value": "dni.identity.ust"
      },
      "IDENTITY_USER_SCOPED_TOKEN_COOKIE_SECRET": {
        "type": "secret",
        "name": "identity_user_scoped_token_cookie_secret"
      },
      "TYPEORM_TYPE": {
        "type": "plain",
        "value": "postgres"
      },
      "TYPEORM_HOST": {
        "type": "plain",
        "value": "euw-dev-213-dai-dipostgres.postgres.database.azure.com"
      },
      "TYPEORM_PORT": {
        "type": "plain",
        "value": "5432"
      },
      "TYPEORM_DATABASE": {
        "type": "plain",
        "value": "di_database"
      },
      "TYPEORM_SCHEMA": {
        "type": "plain",
        "value": "dni"
      },
      "TYPEORM_USERNAME": {
        "type": "secret",
        "name": "typeorm_username"
      },
      "TYPEORM_PASSWORD": {
        "type": "secret",
        "name": "typeorm_password"
      },
      "TYPEORM_SSL": {
        "type": "plain",
        "value": "true"
      },
      "TYPEORM_SYNCHRONIZE": {
        "type": "plain",
        "value": "false"
      },
      "TYPEORM_LOGGING": {
        "type": "plain",
        "value": "true"
      },
      "TYPEORM_RUN_MIGRATION": {
        "type": "plain",
        "value": "false"
      },
      "CCMS_RUN_SYNC": {
        "type": "plain",
        "value": "false"
      },
      "CACHE_COLLEAGUE_TTL": {
        "type": "plain",
        "value": "43200"
      },
      "OIDC_GROUPS_ADMIN_ROLE": {
        "type": "plain",
        "value": "GG-UK-TescoGlobal-DiversityAndInclusion-PPE-Admin"
      },
      "OIDC_GROUPS_MANAGER_ROLE": {
        "type": "plain",
        "value": "GG-UK-TescoGlobal-DiversityAndInclusion-PPE-Manager"
      }
    }
  }
}
