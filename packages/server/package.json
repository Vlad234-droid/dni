    "@energon-connectors/core": "^0.7.6",
    "@energon/fetch-client": "^0.10.1",
    "@energon/onelogin": "^1.30.3",
    "@energon/rest-api-definition": "^0.4.1",
    "@energon/splunk-logger": "^0.25.6",
    "@energon/type-utils": "^0.9.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "form-data": "^4.0.0",
    "jest-express": "^1.12.0",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.2",
