{
  "name": "@dni/server",
  "version": "1.0.0",
  "main": "src/app.ts",
  "license": "MIT",
  "scripts": {
    "run:dev": "env-cmd -f .env nodemon --watch \"src/**/*\" -e ts,tsx --exec \"ts-node --files\" src/app.ts",
    "run:prod": "env-cmd --silent --no-override --fallback -f .env.${BUILD_ENV_PARAM} ts-node --transpile-only -- src/app.ts",
    "clean": "rimraf build coverage",
    "clean:all": "yarn clean && rimraf node_modules",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@dni-connectors/colleague-cms-api": "1.0.0",
