{
  "name": "@dni/server",
  "version": "1.0.0",
  "main": "src/app.ts",
  "license": "MIT",
  "scripts": {
    "run:dev": "env-cmd -f .env nodemon --watch \"src/**/*\" -e ts,tsx --exec \"ts-node --files\" src/app.ts",
    "run:prod": "env-cmd -f .env.${BUILD_ENV_PARAM} node ./build/app.js",
    "build": "env-cmd -f .env.${BUILD_ENV_PARAM} tsc --project ./",
    "build:dev": "env-cmd -f .env tsc --project ./",
    "clean": "rimraf build coverage",
    "clean:all": "yarn clean && rimraf node_modules",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "db:docker:compose:up": "docker-compose pull && docker-compose up -d",
    "db:docker:compose:start": "docker-compose start",
    "db:docker:compose:down": "docker-compose down"
  },
  "dependencies": {
    "@dni-connectors/colleague-cms-api": "1.0.0",
    "@dni-connectors/identity-api": "1.0.0",
    "@energon-connectors/contact-api": "^0.3.6",
    "@energon-connectors/core": "^0.7.6",
    "@energon/fetch-client": "^0.10.1",
    "@energon/onelogin": "^1.27.7",
    "@energon/rest-api-definition": "^0.4.1",
    "@energon/splunk-logger": "^0.25.6",
    "@energon/type-utils": "^0.9.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "form-data": "^4.0.0",
    "jest-express": "^1.12.0",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.2",
    "pg": "^8.5.1",
    "pg-hstore": "^2.3.3",
    "redux-first-router": "^2.1.5",
    "sequelize": "^6.6.2",
    "socket.io": "^4.0.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/express-fileupload": "^1.1.6",
    "@types/multer": "^1.4.5",
    "@types/node": "^14.14.21",
    "@types/pg": "^7.14.10",
    "@types/validator": "^13.1.3",
    "dotenv": "^8.2.0",
    "env-cmd": "^10.1.0",
    "nodemon": "^2.0.7",
    "sequelize-cli": "^6.2.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  }
}
