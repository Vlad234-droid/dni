    "@types/multer": "^1.4.5",
    "@types/node": "^14.14.21",
    "@types/validator": "^13.1.3",
    "dotenv": "^8.2.0",
    "env-cmd": "^10.1.0",
    "nodemon": "^2.0.7",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  }
