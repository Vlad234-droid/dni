{
  "name": "@dni/mock-server",
  "version": "1.0.0",
  "main": "src/app.ts",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build:prod": "tsc -p .",
    "run:dev": "cross-env NODE_PATH=./src nodemon --watch \"src/**/*\" -e ts,tsx --exec \"ts-node --files\" src/app.ts",
    "run:mocked:prod": "cross-env NODE_PATH=./src ts-node --transpile-only --project tsconfig.json -- src/app.ts",
    "clean": "rimraf build coverage storybook-static .eslintcache",
    "clean:all": "yarn clean && rimraf node_modules",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@dni-connectors/colleague-api": "1.0.0",
    "@dni-connectors/colleague-cms-api": "1.0.0",
    "@dni-connectors/confirmit-api": "1.0.0",
    "@dni-connectors/identity-api": "1.0.0",
    "@energon-connectors/core": "^0.8.7",
    "@energon/fetch-client": "^0.12.7",
    "@energon/rest-api-provider": "^0.4.7",
    "ajv": "^7.1.1",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "faker": "^5.4.0",
    "luxon": "^1.26.0",
    "mockdate": "^3.0.2",
    "ts-node": "^9.1.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/faker": "^5.1.7",
    "@types/luxon": "^1.26.0",
    "@types/node": "^14.14.21",
    "@types/supertest": "^2.0.10",
    "nodemon": "^2.0.7",
    "supertest": "^6.1.3",
    "typescript": "^4.1.3"
  }
}
