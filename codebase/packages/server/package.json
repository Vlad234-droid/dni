    "@types/cli-progress": "^3.9.1",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/express-fileupload": "^1.1.6",
    "@types/multer": "^1.4.5",
    "@types/node": "^14.14.21",
