{
  "name": "@dni/server",
  "version": "0.1.0",
  "license": "MIT",
  "private": "false",
  "files": [
    "build"
  ],
  "main": "./build/app.js",
  "scripts": {
    "clean": "rimraf build coverage .eslintcache",
    "clean:npm": "rimraf node_modules",
    "clean:all": "yarn clean && yarn clean:npm",
    "build:prod": "tsc -p .",
    "build:server": "tsc -p .",
    "start:dev": "cross-env NODE_ENV=development NODE_PORT=4000 env-cmd --silent --no-override --fallback -f .env.development node -r source-map-support/register --inspect build/app.js",
    "start:debug": "cross-env NODE_ENV=production env-cmd --silent --no-override --fallback -f .env.production node -r source-map-support/register --inspect build/app.js",
    "start:prod": "node build/app.js",
    "start:server": "cross-env NODE_ENV=production env-cmd --silent --no-override --fallback -f .env.production node build/app.js",
    "cli:ccms-sync:debug": "cross-env NODE_ENV=development env-cmd --silent --no-override --fallback -f .env.${NODE_ENV} node -r source-map-support/register --inspect build/cli/ccms-sync.js",
    "cli:ccms-sync:prod": "node build/cli/ccms-sync.js",
    "cli:ccms-sync:server": "cross-env NODE_ENV=production env-cmd --silent --no-override --fallback -f .env.${NODE_ENV} node build/cli/ccms-sync.js",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch"
  },
