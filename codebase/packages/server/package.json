    "@energon-connectors/contact-api": "^0.4.7",
    "@energon-connectors/core": "^0.8.7",
    "@energon/fetch-client": "^0.12.7",
    "@energon/rest-api-definition": "^0.5.7",
    "@energon/type-utils": "^0.10.0",
    "chalk": "^4.1.1",
    "cli-progress": "^3.9.1",
