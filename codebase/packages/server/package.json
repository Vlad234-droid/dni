{
  "name": "@dni/server",
  "version": "1.0.0",
  "license": "MIT",
  "private": "false",
  "files": [
    "build"
  ],
  "main": "./build/app.js",
  "scripts": {
    "clean": "rimraf build coverage storybook-static .eslintcache",
    "clean:all": "yarn clean && rimraf node_modules",
    "build:prod": "tsc -p .",
    "run:dev": "cross-env BUILD_ENV=development env-cmd --silent --no-override --fallback -f .env.${BUILD_ENV} nodemon --watch \"src/**/*\" -e ts,tsx --exec \"ts-node --files\" src/app.ts",
    "run:prod": "cross-env BUILD_ENV=production env-cmd --silent --no-override --fallback -f .env.production node build/app.js",
    "debug": "cross-env BUILD_ENV=production env-cmd --silent --no-override --fallback -f .env.production node --inspect build/app.js",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "cli:cms-entities-init:dev": "cross-env BUILD_ENV=development env-cmd --silent --no-override --fallback -f .env.${BUILD_ENV} ts-node --script-mode src/cli/ccms-entities-init.ts",
    "cli:cms-entities-init:prod": "cross-env BUILD_ENV=production env-cmd --silent --no-override --fallback -f .env.${BUILD_ENV} ts-node --script-mode src/cli/ccms-entities-init.ts"
  },
  "dependencies": {
    "@dni-connectors/colleague-api": "1.0.0",
    "@dni-connectors/colleague-cms-api": "1.0.0",
    "@dni-connectors/confirmit-api": "1.0.0",
    "@dni-connectors/contact-api": "1.0.0",
    "@dni-connectors/identity-api": "1.0.0",
    "@dni-connectors/onelogin": "1.0.0",
    "@dni/database": "1.0.0",
    "@energon-connectors/contact-api": "^0.4.7",
    "@energon-connectors/core": "^0.8.7",
    "@energon/fetch-client": "^0.12.7",
    "@energon/rest-api-definition": "^0.5.7",
    "@energon/splunk-logger": "^0.27.6",
    "@energon/type-utils": "^0.10.0",
    "chalk": "^4.1.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "form-data": "^4.0.0",
    "jest-express": "^1.12.0",
    "json-colorizer": "^2.2.2",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.2",
    "node-cache": "^5.1.2",
    "node-schedule": "^1.3.1",
    "pg-hstore": "^2.3.3",
    "pino": "^6.11.3",
    "pino-http": "^5.5.0",
    "pino-pretty": "^5.1.2",
    "puppeteer": "^9.1.0",
    "qs": "^6.10.1",
    "redux-first-router": "^2.1.5",
    "unionize": "^3.1.0",
    "uuid": "^8.3.2",
    "yn": "^4.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/express-fileupload": "^1.1.6",
    "@types/multer": "^1.4.5",
    "@types/node": "^14.14.21",
    "@types/node-schedule": "^1.3.1",
    "@types/pino": "^6.3.9",
    "@types/pino-http": "^5.4.2",
    "@types/uuid": "^8.3.1",
    "@types/validator": "^13.1.3",
    "dotenv": "^8.2.0",
    "env-cmd": "^10.1.0",
    "esbuild": "^0.12.15",
    "nodemon": "^2.0.7",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3",
    "webpack": "^5.45.1",
    "webpack-cli": "^4.7.2"
  }
}
