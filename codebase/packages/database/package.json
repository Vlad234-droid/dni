{
  "name": "@dni/database",
  "version": "0.1.0",
  "license": "MIT",
  "private": true,
  "files": [
    "build"
  ],
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "scripts": {
    "clean": "rimraf build coverage .eslintcache",
    "clean:npm": "rimraf node_modules",
    "clean:all": "yarn clean && yarn clean:npm",
    "build:prod": "cross-env tsc -p . && mkdir -p ./build/migrations/data && cp -r ./src/migrations/data/* ./build/migrations/data",
    "typeorm:dev": "ts-node --transpile-only ../../node_modules/typeorm/cli.js --config ./src/ormconfig.ts",
    "typeorm": "node ../../node_modules/typeorm/cli.js --config ./build/ormconfig.js",
    "entity:create": "yarn typeorm entity:create -n",
    "subscriber:create": "yarn typeorm subscriber:create -n",
    "cache:clear": "yarn typeorm cache:clear",
    "migration:generate": "yarn typeorm migration:generate -n",
    "migration:create": "yarn typeorm migration:create -n",
    "migration:show": "yarn typeorm migration:show",
    "migration:run": "yarn typeorm  migration:run",
    "migration:revert": "yarn typeorm migration:revert",
    "query": "yarn typeorm query",
    "schema:sync": "yarn typeorm schema:sync",
    "schema:log": "yarn typeorm schema:log",
    "schema:drop": "yarn typeorm schema:drop",
    "version": "yarn typeorm version"
  },
  "dependencies": {
    "chalk": "^4.1.1",
    "csv-parser": "^3.0.0",
    "dotenv": "^8.2.0",
    "lodash": "^4.17.21",
    "pg": "^8.6.0",
    "pg-copy-streams": "^5.1.1",
    "pino": "^6.11.3",
    "reflect-metadata": "^0.1.13",
    "strip-bom-stream": "^4.0.0",
    "typeorm": "^0.2.32",
    "yn": "^4.0.0"
  },
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/node": "^14.14.41",
    "@types/pg": "^7.14.11",
    "@types/pg-copy-streams": "^1.2.1",
    "cross-env": "^7.0.3",
    "env-cmd": "^10.1.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  }
}
