{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "noImplicitAny": false,
    "noEmit": true,
    "moduleResolution": "node",
    "lib": ["dom", "dom.iterable", "esnext"],
    "typeRoots": ["./src/types", "./node_modules/@types", "../../node_modules/@types"],
    "baseUrl": "./src",
    "jsx": "react-jsx"
  },
  "include": ["./src/**/*"],
  "exclude": ["node_modules", "**/*.spec.ts", "./src/setupProxy.js"]
}
